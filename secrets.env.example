# Local secrets file example
# Format: KEY=value

# Environment profile: dev, staging, prod, laptop
# When set to "dev", this will map to "cyberpower" hostname for Pure Translation
ZSH_ENV_PROFILE=dev

# Pure Translation environment hostname mapping
# When ZSH_ENV_PROFILE=dev, set this to "cyberpower" to use production-like settings
PT_ENV_HOSTNAME=cyberpower

FEC_API_KEY=replace_me
SERVICE_TOKEN=replace_me
